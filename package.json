{
  "name": "front",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "pushtest": "vitest run -u",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "prettier": "prettier -w ./src",
    "cssdev": "postcss src/styles/main.css -o public/main.css",
    "cssprod": "cross-env NODE_ENV=production postcss src/styles/main.css -o public/main.css",
    "push": "run-s cssdev prettier lint pushtest",
    "prod": "run-s cssprod lint prettier pushtest"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@kyvg/vue3-notification": "^3.0.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^7.1.0",
    "axios": "^1.5.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "compressorjs": "^1.2.1",
    "net": "^1.0.2",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "scrollama": "^3.2.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "vue": "^3.3.4",
    "vue-cookies": "^1.8.3",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vuelidate": "^0.7.7"
  },
  "devDependencies": {
    "@capacitor/android": "^5.5.0",
    "@capacitor/cli": "^5.5.0",
    "@capacitor/core": "^5.5.0",
    "@pinia/testing": "^0.1.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cssom": "^0.5.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^12.9.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.6"
  }
}
